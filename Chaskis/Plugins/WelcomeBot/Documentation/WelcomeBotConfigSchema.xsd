<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="WelcomeBotConfigSchema"
    targetNamespace="https://files.shendrick.net/projects/chaskis/schemas/welcomebotconfig/2018/welcomebotconfigschema.xsd"
